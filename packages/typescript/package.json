{
  "name": "@nebula/proof-kit",
  "version": "1.0.0",
  "description": "Verification toolkit for Nebula cryptographic proofs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cryptography",
    "proofs",
    "verification",
    "merkle",
    "ed25519",
    "nebula"
  ],
  "author": "Aeonica Labs <security@nebulaguard.net>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aeonica-Labs/nebula-proof-kit.git",
    "directory": "packages/typescript"
  },
  "bugs": {
    "url": "https://github.com/Aeonica-Labs/nebula-proof-kit/issues"
  },
  "homepage": "https://github.com/Aeonica-Labs/nebula-proof-kit#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  }
}
